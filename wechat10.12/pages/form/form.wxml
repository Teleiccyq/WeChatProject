<view class="weui-form__control-area">
	<view class="weui-cells__group weui-cells__group_form">
		<view class="weui-cells weui-cells_form">
			<view class="weui-cell weui-cell_active">
				<view class="weui-cell__hd"><label class="weui-label">姓名</label></view>
				<view class="weui-cell__bd">
					<input class="weui-input" placeholder-class="weui-input__placeholder" />
				</view>
			</view>
			<view class="weui-cell weui-cell_active">
				<view class="weui-cell__hd">
					<label class="weui-label">昵称性别</label>
				</view>
				<view class="weui-cell__bd">
					<radio-group class="radio-group" bindchange="radioChange">
						<radio class="radio">
							<text>男</text>
						</radio>
						<radio class="radio">
							<text>女</text>
						</radio>
					</radio-group>
				</view>
			</view>
			<view class="weui-cell weui-cell_active">
				<view class="weui-cell__hd">
					<label class="weui-label">年龄</label>
				</view>
				<view class="weui-cell__bd">
					<view class="section section_gap">
						<view class="body-view">
							<slider bindchange="slider4change" min="0" max="90" show-value />
						</view>
					</view>
				</view>
			</view>
			<view class="weui-cell weui-cell_active">
				<view class="weui-cell__hd"><label class="weui-label">兴趣爱好</label></view>
				<view class="group">
					<checkbox-group>
						<checkbox>
							<text>篮球</text>
						</checkbox>
						<checkbox>
							<text>插花</text>
						</checkbox>
					</checkbox-group>
					<checkbox-group>
						<checkbox>
							<text>动漫</text>
						</checkbox>
						<checkbox>
							<text>游泳</text>
						</checkbox>
					</checkbox-group>
				</view>
			</view>
			<view class="weui-cell weui-cell_active">
				<view class="weui-cell__hd"><label class="weui-label">联系电话</label></view>
				<view class="weui-cell__bd">
					<input class="weui-input" placeholder-class="weui-input__placeholder" />
				</view>
			</view>
			<view class="weui-cell weui-cell_active">
				<view class="weui-cell__hd"><label class="weui-label">出生年月</label></view>
				<view class="weui-cell__bd">
					<picker mode="date" value="{{date}}" start="2015-09-01" end="2017-09-01" bindchange="bindDateChange">
						<view class="picker">
							点击选择日期: {{date}}
						</view>
					</picker>
				</view>
			</view>
			<view class="weui-cell weui-cell_active">
				<view class="weui-cell__hd"><label class="weui-label">家庭住址</label></view>
				<view class="weui-cell__bd">
					<view class="section">
							<picker mode="date" value="{{date}}" start="2015-09-01" end="2017-09-01" bindchange="bindDateChange">
						<view class="picker">
							点击选择日期: {{date}}
						</view>
					</picker>
					</view>
				</view>
			</view>
		</view>
	</view>
</view>
<view class="weui-form__opr-area">
	<view class="weui-btn weui-btn_primary">确定</view>
</view>